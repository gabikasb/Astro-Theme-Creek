        text-decoration: none;
    }
    .data {
        text-align: justify;
    }
    .emoji {
        font-size: xx-large;
    }
    .cta[data-descr] {
        position: relative;
        text-decoration: none;
        color: #000;
        cursor: help;
        font-weight: 700;
        }
    .cta[data-descr]:hover::after,
    .cta[data-descr]:focus::after {
        content: attr(data-descr);
        position: absolute;
        left: 0;
        top: 24px;
        min-width: 200px;
        border: 1px #aaaaaa solid;
        border-radius: 10px;
        background-color: #ffffcc;
        padding: 12px;
        color: #000000;
        font-size: 14px;
